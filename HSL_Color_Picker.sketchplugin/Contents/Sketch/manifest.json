{
    "name": "HSL Color Picker",
    "description": "HSL Color Picker for Sketch.",
    "author": "Ashung Hung",
    "email": "Ashung.hung@gmail.com",
    "homepage": "https://github.com/Ashung/HSL_Color_Picker",
    "version": "0.1",
    "identifier": "com.ashung.hung.hsl_color_picker",
    "compatibleVersion": 3,
    "bundleVersion": 1,
    "commands": [
        {
            "name": "Color Picker 🎨",
            "identifier": "hsl_color_picker",
            "handler": "onRun",
            "script": "hsl_color_picker.cocoascript"
        },
        {
            "name": "Hue Plus (H+10)",
            "identifier": "hue_plus",
            "handler": "huePlus",
            "script": "hsl_color_picker.cocoascript"
        },
        {
            "name": "Hue Minus (H-10)",
            "identifier": "hue_minus",
            "handler": "hueMinus",
            "script": "hsl_color_picker.cocoascript"
        },
        {
            "name": "More Dark (L-5) 👦🏿",
            "identifier": "more_dark",
            "handler": "moreDark",
            "script": "hsl_color_picker.cocoascript"
        },
        {
            "name": "More Light (L+5) 👦🏼",
            "identifier": "more_light",
            "handler": "moreLight",
            "script": "hsl_color_picker.cocoascript"
        },
        {
            "name": "More Bright (S+5) 🙈",
            "identifier": "more_bright",
            "handler": "moreBright",
            "script": "hsl_color_picker.cocoascript"
        },
        {
            "name": "More Gray (S-5) 🙉",
            "identifier": "more_gray",
            "handler": "moreGray",
            "script": "hsl_color_picker.cocoascript"
        }
    ],
    "menu": {
        "isRoot": false,
        "title": "HSL Color Picker",
        "items": [
            "hsl_color_picker",
            "-",
            "hue_plus",
            "hue_minus",
            "more_bright",
            "more_gray",
            "more_light",
            "more_dark"
        ]
    }
}
