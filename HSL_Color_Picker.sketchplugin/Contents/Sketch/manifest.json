{
    "name": "HSL Color Picker",
    "description": "HSL Color Picker for Sketch.",
    "author": "Ashung Hung",
    "email": "Ashung.hung@gmail.com",
    "homepage": "https://github.com/Ashung/HSL_Color_Picker",
    "version": "1.3",
    "identifier": "com.ashung.hung.hsl_color_picker",
    "compatibleVersion": "3",
    "bundleVersion": "1",
    "commands": [
        {
            "name": "Color Picker 🎨",
            "identifier": "hsl_color_picker",
            "handler": "onRun",
            "script": "hsl_color_picker.cocoascript"
        },
        {
            "name": "Hue Plus (H+10) 🏳️‍🌈",
            "identifier": "hue_plus",
            "handler": "huePlus",
            "script": "hsl_color_picker.cocoascript"
        },
        {
            "name": "Hue Minus (H-10) 🏳️‍🌈",
            "identifier": "hue_minus",
            "handler": "hueMinus",
            "script": "hsl_color_picker.cocoascript"
        },
        {
            "name": "More Dark (L-5) 👦🏿",
            "identifier": "more_dark",
            "handler": "moreDark",
            "script": "hsl_color_picker.cocoascript"
        },
        {
            "name": "More Light (L+5) 👦🏼",
            "identifier": "more_light",
            "handler": "moreLight",
            "script": "hsl_color_picker.cocoascript"
        },
        {
            "name": "More Bright (S+5) 🙈",
            "identifier": "more_bright",
            "handler": "moreBright",
            "script": "hsl_color_picker.cocoascript"
        },
        {
            "name": "More Gray (S-5) 🙉",
            "identifier": "more_gray",
            "handler": "moreGray",
            "script": "hsl_color_picker.cocoascript"
        },
        {
            "name": "Web Site 🌏",
            "identifier": "web_site",
            "handler": "website",
            "script": "hsl_color_picker.cocoascript"
        },
        {
            "name": "Report Issue 🐞",
            "identifier": "report_issue",
            "handler": "reportIssue",
            "script": "hsl_color_picker.cocoascript"
        },
        {
            "name": "Donate via PayPal 💵 ",
            "identifier": "donate_via_paypal",
            "handler": "donateViaPaypal",
            "script": "hsl_color_picker.cocoascript"
        },
        {
            "name": "微信扫码打赏 💵 ",
            "identifier": "donate_via_wechat",
            "handler": "donateViaWechat",
            "script": "hsl_color_picker.cocoascript"
        },
        {
            "name": "支付宝扫码打赏 💵 ",
            "identifier": "donate_via_alipay",
            "handler": "donateViaAlipay",
            "script": "hsl_color_picker.cocoascript"
        }
    ],
    "menu": {
        "isRoot": false,
        "title": "HSL Color Picker",
        "items": [
            "hsl_color_picker",
            "-",
            "hue_plus",
            "hue_minus",
            "more_bright",
            "more_gray",
            "more_light",
            "more_dark",
            "-",
            {
                "title": "Help",
                "items": [
                    "web_site",
                    "report_issue",
                    "-",
                    "donate_via_paypal",
                    "donate_via_wechat",
                    "donate_via_alipay"
                ]
            }
        ]
    }
}
